<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_gray"
    tools:context="com.inspection.fragments.FragmentARRAVFacility">

    <LinearLayout
        android:id="@+id/facll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="10">

        <TextView
            android:id="@+id/titleTextView"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_gravity="center_vertical"
            android:layout_weight="8"
            android:gravity="left"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="Facility"
            android:textSize="18dp"
            android:textStyle="bold" />


    </LinearLayout>

    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/facll"
        android:layout_margin="10dp"
        android:background="@color/white"
        android:orientation="vertical"
        android:paddingBottom="10dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="5dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="10dp"
                android:stretchColumns="5dp">

                <TableRow>

                    <TextView
                        android:id="@+id/contractStatusTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_gravity="center_vertical"
                        android:text="Contract Status: "
                        android:textStyle="bold" />

                    <RadioGroup
                        android:id="@+id/contractStatusRadioGroup"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:layout_toRightOf="@id/contractStatusTextView"
                        android:orientation="horizontal">

                        <RadioButton
                            android:id="@+id/activeRadioButton"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:checked="true"
                            android:text="Active" />

                        <RadioButton
                            android:id="@+id/inActiveRadioButton"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="InActive" />


                    </RadioGroup>
                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/contract_number_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:text="Contract Number: "
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/contract_number_textviewVal"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:layout_column="1"
                        android:textSize="12sp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:background="@drawable/textview_background"
                        android:ems="6"
                        android:padding="5dp"
                        android:text="test" />

                    <TextView
                        android:id="@+id/contractTypeTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="2"
                        android:text="Contract Type: "
                        android:textStyle="bold" />

                    <Spinner
                        android:id="@+id/contractTypeValueSpinner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="3"
                        android:entries="@array/contractTypesEntries"
                        android:text="test" />

                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/providertype_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:text="Provider Type: "
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:orientation="vertical">

                            <CheckBox
                                android:id="@+id/aarCheckBox"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="AAR" />

                            <CheckBox
                                android:id="@+id/aabCheckBox"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="AAB" />

                            <CheckBox
                                android:id="@+id/aagCheckBox"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="AAG" />

                            <CheckBox
                                android:id="@+id/cogCheckBox"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="COG" />

                            <CheckBox
                                android:id="@+id/corCheckBox"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="COR" />

                            <CheckBox
                                android:id="@+id/ersCheckBox"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="ERS" />

                            <CheckBox
                                android:id="@+id/mprCheckBox"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="MPR" />

                            <CheckBox
                                android:id="@+id/pspCheckBox"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="PSP" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:orientation="vertical">

                            <EditText
                                android:id="@+id/aarEditText"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:layout_gravity="left"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:background="@drawable/textview_background"
                                android:ems="10"
                                android:padding="5dp"
                                android:textSize="12sp" />


                            <EditText
                                android:id="@+id/aabEditText"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:layout_gravity="left"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:background="@drawable/textview_background"
                                android:ems="10"
                                android:padding="5dp"
                                android:text=""
                                android:textSize="12sp" />

                            <EditText
                                android:id="@+id/aagEditText"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:layout_gravity="left"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:background="@drawable/textview_background"
                                android:ems="10"
                                android:padding="5dp"
                                android:text=""
                                android:textSize="12sp" />

                            <EditText
                                android:id="@+id/cogEditText"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:layout_gravity="left"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:background="@drawable/textview_background"
                                android:ems="10"
                                android:padding="5dp"
                                android:text=""
                                android:textSize="12sp" />

                            <EditText
                                android:id="@+id/corEditText"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:layout_gravity="left"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"

                                android:background="@drawable/textview_background"
                                android:ems="10"
                                android:padding="5dp"
                                android:text=""
                                android:textSize="12sp" />

                            <EditText
                                android:id="@+id/ersEditText"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:layout_gravity="left"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:background="@drawable/textview_background"
                                android:ems="10"
                                android:padding="5dp"
                                android:text=""
                                android:textSize="12sp" />

                            <EditText
                                android:id="@+id/mprEditText"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:layout_gravity="left"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:background="@drawable/textview_background"
                                android:ems="10"
                                android:padding="5dp"
                                android:text=""
                                android:textSize="12sp" />

                            <EditText
                                android:id="@+id/pspEditText"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:layout_gravity="left"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:background="@drawable/textview_background"
                                android:ems="10"
                                android:padding="5dp"
                                android:text=""
                                android:textSize="12sp" />
                        </LinearLayout>

                    </LinearLayout>
                </TableRow>


                <TableRow>

                    <TextView
                        android:id="@+id/office_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Office: "
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/office_textviewVal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/office_textview"
                        android:text="test" />
                </TableRow>


                <TableRow>

                    <TextView
                        android:id="@+id/assignedto_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/officell"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Assigned To: "
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/assignedto_textviewVal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/officell"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/assignedto_textview"
                        android:text="test" />

                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/dba_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/AssignedToll"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="DBA: "
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/dba_textviewVal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/AssignedToll"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/dba_textview"
                        android:text="test" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/entity_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/dball"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Entity Name: "
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/entity_textviewVal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/dball"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/entity_textview"
                        android:text="test" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/bustype_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/entityll"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Business Type: "
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/bustype_textviewVal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/entityll"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/bustype_textview"
                        android:text="test" />
                </TableRow>


                <TableRow>

                    <TextView
                        android:id="@+id/terminationDateTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Termination Date: "
                        android:textStyle="bold" />

                    <Button
                        android:id="@+id/terminationDateButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:layout_gravity="left"
                        android:layout_marginTop="10dp"
                        android:background="@color/transparent"
                        android:text="SELECT DATE"
                        android:textColor="@color/blue"
                        android:textSize="12dp" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/terminationReasonTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Termination Reason: "
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/terminationReasonEditText"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/textview_background"
                        android:text=""
                        android:textSize="12dp" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/terminationReasonTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Termination Comment: "
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/terminationCommentEditText"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:paddingRight="5dp"
                        android:paddingLeft="5dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/textview_background"
                        android:lines="8"
                        android:inputType=""
                        android:text=""
                        android:scrollHorizontally="false"
                        android:textSize="12dp" />
                </TableRow>


                <TableRow>

                    <TextView
                        android:id="@+id/timezone_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Time Zone: "
                        android:textStyle="bold" />

                    <Spinner
                        android:id="@+id/timezone_textviewVal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/timezone_textview"
                        android:text="test" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/payment_methods_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_marginTop="5dp"
                        android:text="Payment Methods: "
                        android:textStyle="bold" />

                    <com.google.android.flexbox.FlexboxLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_span="2"
                        app:flexWrap="wrap">

                        <CheckBox
                            android:id="@+id/americanexpress_checkbox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:clickable="true"
                            android:padding="5dp"
                            android:text="American Express" />

                        <CheckBox
                            android:id="@+id/cash_checkbox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:padding="5dp"
                            android:clickable="true"
                            android:text="Cash" />

                        <CheckBox
                            android:id="@+id/check_checkbox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:clickable="true"
                            android:padding="5dp"
                            android:text="Check" />

                        <CheckBox
                            android:id="@+id/debit_checkbox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            app:layout_wrapBefore="true"
                            android:clickable="true"
                            android:padding="5dp"
                            android:text="Debit" />

                        <CheckBox
                            android:id="@+id/discover_checkbox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:clickable="true"
                            android:padding="5dp"
                            android:text="Discover" />

                        <CheckBox
                            android:id="@+id/goodyear_checkbox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:clickable="true"
                            android:padding="5dp"
                            android:text="Goodyear Credit Card" />

                        <CheckBox
                            android:id="@+id/mastercard_checkbox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            app:layout_wrapBefore="true"
                            android:padding="5dp"
                            android:clickable="true"
                            android:text="Master Card" />

                        <CheckBox
                            android:id="@+id/paypal_checkbox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:clickable="true"
                            android:text="PayPal" />

                        <CheckBox
                            android:id="@+id/visa_checkbox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:clickable="true"
                            android:padding="5dp"
                            android:text="Visa" />
                    </com.google.android.flexbox.FlexboxLayout>


                </TableRow>


                <TableRow>

                    <TextView
                        android:id="@+id/website_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Website URL: "
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/website_textviewVal"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/website_textview"
                        android:hint="Enter website URL"
                        android:background="@drawable/textview_background"
                        android:text=""
                        android:padding="5dp"
                        android:textSize="12dp" />
                </TableRow>

                <TableRow>

                    <CheckBox
                        android:id="@+id/wifi_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Wi-Fi Available"
                        android:textStyle="bold" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/website_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/wifi_textview"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Tax-ID: "
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/texno_textviewVal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/wifi_textview"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/website_textview"
                        android:text="test" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/repairorder_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Repair Order Count: "
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/repairorder_textviewVal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/repairorder_textview"
                        android:hint="Enter repair order count"
                        android:inputType="number"
                        android:maxLength="6"
                        android:text=""
                        android:textSize="12dp" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/InspectionMonthTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:text="Inspection Month:"
                        android:textStyle="bold" />

                    <Spinner
                        android:id="@+id/inspectionMonthsSpinner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:entries="@array/inspectionMonths" />


                    <TextView
                        android:id="@+id/InspectionCycleTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="2"
                        android:layout_gravity="center"
                        android:text="Inspection Monthn:"
                        android:textStyle="bold" />

                    <Spinner
                        android:id="@+id/inspectionCycleSpinner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="3"
                        android:layout_gravity="center"
                        android:entries="@array/inspectionCycle" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/availability_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Service Availability:"
                        android:textStyle="bold" />

                    <Spinner
                        android:id="@+id/availability_textviewVal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/availability_textview"
                        android:text="test" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/facilitytype_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="Facility Type: "
                        android:textStyle="bold" />

                    <Spinner
                        android:id="@+id/facilitytype_textviewVal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/facilitytype_textview"
                        android:text="test" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:id="@+id/ARDno_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:text="ARD Number: "
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/ARDno_textviewVal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/ARDno_textview"
                        android:digits="abcdefghijklmnopqrstuvwxyz1234567890"
                        android:hint="Enter ARD Number"
                        android:maxLength="12"
                        android:text=""
                        android:textSize="12dp" />
