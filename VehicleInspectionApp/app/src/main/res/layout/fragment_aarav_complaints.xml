        <TextView
            android:id="@+id/justComRatioTextViewId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:ems="8"
            android:background="@drawable/textview_background"
            />

    </LinearLayout>


